<%= form_for :dump, url: {action: "import_csv"}, html: { multipart: true } do |f| %>
  <table>
    <tr>
      <td>
        <%= f.label :dump_file, "Select a CSV File :" %>
      </td>    
      <td>
        <%= f.file_field :file %>
    </tr>
    <tr>
      <td>
        <%= submit_tag 'Submit' %>
      </td>
    </tr>
<% end %>